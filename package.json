    "simple-debugger": "^1.0.3",
    "simple-e2p": "^1.0.1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.6",
    "eslint": "1.10.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nskazki/simple-pmysql.git"
  },
  "bugs": {
